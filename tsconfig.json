{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"lib": ["ESNext"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"experimentalDecorators": true,
		"baseUrl": "./src",
		"paths": {
			"@/*": ["*"],
			"@modules/*": ["modules/*"],
			"@db/*": ["db/*"],
			"@lib/*": ["lib/*"]
		}
	}
}
